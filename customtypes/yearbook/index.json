{
  "id": "yearbook",
  "label": "yearbook",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "content",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,heading2,strong,em,hyperlink,list-item,o-list-item"
        }
      },
      "color": {
        "type": "Select",
        "config": {
          "label": "color",
          "placeholder": "",
          "options": ["red", "blue", "green", "yellow", "purple", "brown"]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "members": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
